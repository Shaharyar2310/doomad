import matplotlib.pyplot as plt

x = [2,4,2,4,2]
y = [1,3,5,7,9]
plt.plot(x,y)
plt.title("sideways w ")
plt.xlabel("years")
plt.ylabel("sales")

x = [2,4,2,4,2]
y = [1,3,5,7,9]
plt.bar(x,y)
plt.title("sideways w ")
plt.xlabel("years")
plt.ylabel("sales")

x=[1,2,3,4,5,6,7,8,9]
plt.pie(x)
plt.title("sideways w ")
plt.xlabel("years")
plt.ylabel("sales")
